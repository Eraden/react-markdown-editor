{
  "name": "react-markdown-editor",
  "version": "2.0.0",
  "description": "A markdown editor with preview, implemented with React.js and Redux",
  "main": "lib/MarkdownEditor.js",
  "keywords": [
    "react-component",
    "markdown",
    "react",
    "editor",
    "preview",
    "text"
  ],
  "authors": [
    "Jeremy Dagorn <jeremy.dagorn@gmail.com>",
    "Adrian Wo≈∫niak <adrian.wozniak@ita-prog.pl>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eraden/react-markdown-editor",
    "email": "adrian.wozniak@ita-prog.pl"
  },
  "jest": {
    "transform": {
      ".*": "./scripts/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/reflux",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-tools",
      "<rootDir>/node_modules/enzyme"
    ],
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eraden/react-markdown-editor.git"
  },
  "scripts": {
    "build": "NODE_ENV=development WEBPACK_ENV=build webpack --mode=development",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch"
  },
  "dependencies": {
    "@babel/core": "7",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "6",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "7",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.0",
    "font-awesome": "^4.7.0",
    "image-webpack-loader": "^4.6.0",
    "markdown": "^0.5.0",
    "materialize-css": "^1.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "reflux": "^6.4.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
